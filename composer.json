{
  "name": "exozet/behat-utils",
  "description": "The `exozet/behat-utils` provides some traits for easier api testing with behat and mink.",
  "keywords": [
    "behat",
    "mink",
    "bdd",
    "gherkin"
  ],
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Exozet\\Behat\\Utils\\Base\\": "src",
      "Exozet\\Behat\\Utils\\Test\\": "features/bootstrap"
    }
  },
  "minimum-stability": "stable",
  "require": {
    "behat/gherkin": "*",
    "behat/mink": "^1.7",
    "phpunit/phpunit": "^7"
  }
}
